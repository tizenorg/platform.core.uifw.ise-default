<?xml version="1.0"?>
<magnifier_configure>
  <magnifier_style>label_only</magnifier_style>
  <width>100</width>
  <height>80</height>
  <label_area>
    <left>0</left>
    <top>0</top>
    <right>100</right>
    <bottom>80</bottom>
  </label_area>
  <background_image_path>
    <image state="normal">w_sip_3x4_popup_bg_02.#.png</image>
    <image state="shift">w_sip_3x4_popup_bg_02.#.png</image>
    <image state="lock">w_sip_3x4_popup_bg_02.#.png</image>
    <image state="longkey">w_sip_3x4_popup_bg_02.#.png</image>
  </background_image_path>
  <use_window>true</use_window>
  <label_type>MAGNIFIER</label_type>
  <padding>
    <x>-10</x>
    <y>10</y>
  </padding>
  <show_shift_label>false</show_shift_label>
</magnifier_configure>
