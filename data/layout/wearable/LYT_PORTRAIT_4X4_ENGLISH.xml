<?xml version="1.0" encoding="UTF-8"?>
<layout width="360" height="240" key_width="80" key_height="60" vibe_style="DEFAULT" sound_style="DEFAULT" label_type="3X4_DEFAULT">
  <image_path>
    <button_normal>B09_panel.png</button_normal>
  </image_path>
  <background_image>
    <rec button="normal">button/B09_key_btn_01.png</rec>
    <rec button="pressed">button/B09_key_btn_press.png</rec>
    <rec button="disabled">button/B09_key_btn_01.png</rec>
  </background_image>
  <row>
    <key key_type="string" button_type="multitap" long_key_value="1" width="100">
      <label>
        <rec multi="0">.,?!</rec>
        <rec multi="1">1</rec>
      </label>
      <key_value>
        <rec multichar_state="0">.</rec>
        <rec multichar_state="1">,</rec>
        <rec multichar_state="2">?</rec>
        <rec multichar_state="3">!</rec>
      </key_value>
    </key>
    <key key_type="string" button_type="multitap" long_key_value="2">
      <label>
        <rec multi="0" auto_upper="true">abc</rec>
        <rec multi="1">2</rec>
      </label>
      <key_value>
        <rec auto_upper="true" multichar_state="0">a</rec>
        <rec auto_upper="true" multichar_state="1">b</rec>
        <rec auto_upper="true" multichar_state="2">c</rec>
      </key_value>
    </key>
    <key key_type="string" button_type="multitap" long_key_value="3">
      <label>
        <rec multi="0" auto_upper="true">def</rec>
        <rec multi="1">3</rec>
      </label>
      <key_value>
        <rec auto_upper="true" multichar_state="0">d</rec>
        <rec auto_upper="true" multichar_state="1">e</rec>
        <rec auto_upper="true" multichar_state="2">f</rec>
      </key_value>
    </key>
    <key key_type="control" use_repeat_key="true" width="100">
      <image_label>
        <rec button="normal">B09_icon_back_nor_65x65.png</rec>
        <rec button="pressed">B09_icon_back_press_65x65.png</rec>
      </image_label>
      <background_image>
        <rec button="normal">button/B09_key_btn_02.png</rec>
        <rec button="pressed">button/B09_key_btn_press.png</rec>
        <rec button="disabled">button/B09_key_btn_02.png</rec>
      </background_image>
      <key_value>
        <rec>BackSpace</rec>
      </key_value>
    </key>
  </row>
  <row>
    <key key_type="string" button_type="multitap" long_key_value="4" width="100">
      <label>
        <rec multi="0" auto_upper="true">ghi</rec>
        <rec multi="1">4</rec>
      </label>
      <key_value>
        <rec auto_upper="true" multichar_state="0">g</rec>
        <rec auto_upper="true" multichar_state="1">h</rec>
        <rec auto_upper="true" multichar_state="2">i</rec>
      </key_value>
    </key>
    <key key_type="string" button_type="multitap" long_key_value="5">
      <label>
        <rec multi="0" auto_upper="true">jkl</rec>
        <rec multi="1">5</rec>
      </label>
      <key_value>
        <rec auto_upper="true" multichar_state="0">j</rec>
        <rec auto_upper="true" multichar_state="1">k</rec>
        <rec auto_upper="true" multichar_state="2">l</rec>
      </key_value>
    </key>
    <key key_type="string" button_type="multitap" long_key_value="6">
      <label>
        <rec multi="0" auto_upper="true">mno</rec>
        <rec multi="1">6</rec>
      </label>
      <key_value>
        <rec auto_upper="true" multichar_state="0">m</rec>
        <rec auto_upper="true" multichar_state="1">n</rec>
        <rec auto_upper="true" multichar_state="2">o</rec>
      </key_value>
    </key>
    <key key_type="control" width="100">
      <image_label>
        <rec button="normal">B09_icon_enter.png</rec>
        <rec button="pressed">B09_icon_enter_press.png</rec>
        <rec button="disabled">B09_icon_enter_disabled.png</rec>
      </image_label>
      <background_image>
        <rec button="normal">button/B09_key_btn_02.png</rec>
        <rec button="pressed">button/B09_key_btn_press.png</rec>
        <rec button="disabled">button/B09_key_btn_02.png</rec>
      </background_image>
      <key_value>
        <rec>Enter</rec>
      </key_value>
    </key>
  </row>
  <row>
    <key key_type="string" button_type="multitap" long_key_value="7" width="100">
      <label>
        <rec multi="0" auto_upper="true">pqrs</rec>
        <rec multi="1">7</rec>
      </label>
      <key_value>
        <rec auto_upper="true" multichar_state="0">p</rec>
        <rec auto_upper="true" multichar_state="1">q</rec>
        <rec auto_upper="true" multichar_state="2">r</rec>
        <rec auto_upper="true" multichar_state="3">s</rec>
      </key_value>
    </key>
    <key key_type="string" button_type="multitap" long_key_value="8">
      <label>
        <rec multi="0" auto_upper="true">tuv</rec>
        <rec multi="1">8</rec>
      </label>
      <key_value>
        <rec auto_upper="true" multichar_state="0">t</rec>
        <rec auto_upper="true" multichar_state="1">u</rec>
        <rec auto_upper="true" multichar_state="2">v</rec>
      </key_value>
    </key>
    <key key_type="string" button_type="multitap" long_key_value="9">
      <label>
        <rec multi="0" auto_upper="true">wxyz</rec>
        <rec multi="1">9</rec>
      </label>
      <key_value>
        <rec auto_upper="true" multichar_state="0">w</rec>
        <rec auto_upper="true" multichar_state="1">x</rec>
        <rec auto_upper="true" multichar_state="2">y</rec>
        <rec auto_upper="true" multichar_state="3">z</rec>
      </key_value>
    </key>
    <key key_type="control" width="100">
      <image_label>
        <rec button="normal">B09_icon_shift_50x50.png</rec>
        <rec button="pressed">B09_icon_shift_50x50_press.png</rec>
        <rec button="disabled">B09_icon_shift_50x50_dim.png</rec>
      </image_label>
      <background_image>
        <rec button="normal">button/B09_key_btn_02.png</rec>
        <rec button="pressed">button/B09_key_btn_press.png</rec>
        <rec button="disabled">button/B09_key_btn_02.png</rec>
      </background_image>
      <key_value>
        <rec>Shift</rec>
      </key_value>
    </key>
  </row>
  <row>
    <key key_type="modechange" width="120">
      <label>
        <rec>?123</rec>
      </label>
      <key_value>
        <rec>SYM_4X4_1</rec>
      </key_value>
    </key>
    <key key_type="string" width="120">
      <label>
        <rec>0</rec>
      </label>
      <key_value>
        <rec>0</rec>
      </key_value>
    </key>
    <key key_type="control" width="120">
      <image_label>
        <rec button="normal">B09_icon_space.png</rec>
        <rec button="pressed">B09_icon_space_press.png</rec>
        <rec button="disabled">B09_icon_space_dim.png</rec>
      </image_label>
      <key_value>
        <rec>Space</rec>
      </key_value>
    </key>
  </row>
</layout>
