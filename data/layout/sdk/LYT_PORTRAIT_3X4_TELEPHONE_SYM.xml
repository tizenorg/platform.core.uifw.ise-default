<?xml version="1.0" encoding="UTF-8"?>
<layout width="720" height="460" key_width="165" key_height="100" key_spacing="12" row_spacing="8" hit_left="4" hit_right="4" hit_top="4" hit_bottom="4" label_type="3X4_PHONE" vibe_style="DEFAULT" sound_style="DEFAULT">
  <image_path>
    <button_normal>B09_panel.png</button_normal>
  </image_path>
  <background_image>
    <rec button="normal">button/B09_key_btn_01.png</rec>
    <rec button="pressed">button/B09_key_btn_press.png</rec>
    <rec button="disabled">button/B09_key_btn_01.png</rec>
  </background_image>
  <row x="12" y="12">
    <key>
      <label>
        <rec>(</rec>
      </label>
    </key>
    <key>
      <label>
        <rec>/</rec>
      </label>
    </key>
    <key>
      <label>
        <rec>)</rec>
      </label>
    </key>
    <key button_type="selfish" key_type="control" use_magnifier="true" use_repeat_key="true">
      <image_label>
        <rec button="normal">B09_icon_back.png</rec>
        <rec button="pressed">B09_icon_back_press.png</rec>
      </image_label>
      <background_image>
        <rec button="normal">button/B09_key_btn_02.png</rec>
        <rec button="pressed">button/B09_key_btn_press.png</rec>
        <rec button="disabled">button/B09_key_btn_02.png</rec>
      </background_image>
      <key_value>
        <rec>BackSpace</rec>
      </key_value>
    </key>
  </row>
  <row x="12">
    <key>
      <label>
        <rec>N</rec>
      </label>
    </key>
    <key>
      <label>
        <rec>Pause(,)</rec>
      </label>
      <key_value>
        <rec>,</rec>
      </key_value>
    </key>
    <key>
      <label>
        <rec>.</rec>
      </label>
    </key>
    <key key_type="control" custom_id="Enter">
      <image_label>
        <rec button="normal">B09_icon_enter.png</rec>
        <rec button="pressed">B09_icon_enter_press.png</rec>
        <rec button="disabled">B09_icon_enter_disabled.png</rec>
      </image_label>
      <background_image>
        <rec button="normal">button/B09_key_btn_02.png</rec>
        <rec button="pressed">button/B09_key_btn_press.png</rec>
        <rec button="disabled">button/B09_key_btn_02.png</rec>
      </background_image>
      <key_value>
        <rec>Enter</rec>
      </key_value>
    </key>
  </row>
  <row x="12">
    <key>
      <label>
        <rec>*</rec>
      </label>
    </key>
    <key>
      <label>
        <rec>Wait(;)</rec>
      </label>
      <key_value>
        <rec>;</rec>
      </key_value>
    </key>
    <key>
      <label>
        <rec>#</rec>
      </label>
    </key>
    <key button_type="selfish" key_type="modechange">
      <label>
        <rec>123</rec>
      </label>
      <background_image>
        <rec button="normal">button/B09_key_btn_02.png</rec>
        <rec button="pressed">button/B09_key_btn_press.png</rec>
        <rec button="disabled">button/B09_key_btn_02.png</rec>
      </background_image>
      <key_value>
        <rec>PHONE_3X4</rec>
      </key_value>
    </key>
  </row>
  <row x="12">
    <key>
      <background_image>
        <rec button="normal">button/B09_key_btn_02.png</rec>
        <rec button="pressed">button/B09_key_btn_press.png</rec>
        <rec button="disabled">button/B09_key_btn_02.png</rec>
      </background_image>
      <label>
        <rec>-</rec>
      </label>
    </key>
    <key long_key_value="+">
      <label>
        <rec>+</rec>
      </label>
    </key>
    <key button_type="direction" key_type="control" hit_left="17" hit_right="17" label_type="QTY_SPACE" image_label_type="IMAGE_SPACE">
      <image_label>
        <rec button="normal">B09_icon_space.png</rec>
        <rec button="pressed">B09_icon_space_press.png</rec>
        <rec button="disabled">B09_icon_space_dim.png</rec>
      </image_label>
      <background_image>
        <rec button="normal">button/B09_key_btn_02.png</rec>
        <rec button="pressed">button/B09_key_btn_press.png</rec>
        <rec button="disabled">button/B09_key_btn_02.png</rec>
      </background_image>
      <key_value>
        <rec>Space</rec>
      </key_value>
    </key>
    <key button_type="selfish">
      <image_label>
        <rec button="normal">setting icon/B09_icon_setting.png</rec>
        <rec button="pressed">setting icon/B09_icon_setting_press.png</rec>
        <rec button="disabled">setting icon/B09_icon_setting_dim.png</rec>
      </image_label>
      <background_image>
        <rec button="normal">button/B09_key_btn_02.png</rec>
        <rec button="pressed">button/B09_key_btn_press.png</rec>
        <rec button="disabled">button/B09_key_btn_02.png</rec>
      </background_image>
    </key>
  </row>
</layout>
